@{
    Layout = "";

    SentinelMobile.Models.User user = Session != null ? (SentinelMobile.Models.User)Session["SentinelUser"] : null;
    if (user != null)
    {
        System.Threading.Thread.CurrentThread.CurrentUICulture = new System.Globalization.CultureInfo(user.DefaultLanguage);
    }
}

@for (int i = (ViewBag.PageIndex - 1) * ViewBag.PageSize; i < ViewBag.DsOrganizations.Tables[0].Rows.Count && i < ViewBag.PageIndex * ViewBag.PageSize; i++)    
{
    <li>
        <a href="SuperOrganizationLogin?organizationId=@ViewBag.DsOrganizations.Tables[0].Rows[i]["OrganizationId"]&ticks=@DateTime.Now.Ticks.ToString()" data-ajax="false">
            @ViewBag.DsOrganizations.Tables[0].Rows[i]["OrganizationName"]
        </a>
    </li>
}
