        <style type="text/css">
            .fixedheader
            {
                position:fixed !important;
                width:100%;
                z-index:1000;
                left: 0;
                top: 0;                    
            }

            .ui-content
            {
                padding:65px 15px;
            }

            .ui-dialog-contain
            {
                margin-top: 30px !important;
            }

            #fleetviewhome
            {
                padding-bottom: 52px;
            }
        </style>
        
    <link rel="stylesheet" href="/mobile/Scripts/Openlayers/OpenLayers-2.11/theme/default/style.css" type="text/css" />
    <link rel="stylesheet" href="/mobile/Content/style.mobile-jq.css" type="text/css" />
    <script src="http://maps.googleapis.com/maps/api/js?v=3&client=gme-bsmwirelessinc&sensor=true"></script>
    <script type="text/javascript" src="/mobile/Scripts/Openlayers/OpenLayers-2.11/OpenLayers-mobile.js"></script>
    <script type="text/javascript" src="/mobile/Scripts/mobile-base.js?v=20130412122519"></script>
