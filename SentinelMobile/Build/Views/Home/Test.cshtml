    <body>
       

        
    <div data-role="page" id="vehicleList">
            <!--<div data-role="header" data-theme="g" id="vehicleListHeader" data-position="fixed" data-tap-toggle="false">-->
            <div data-role="header" data-theme="g" id="vehicleListHeader" class="fixedheader">
                <h2>Vehicles</h2>
                <div data-role="controlgroup" data-type="horizontal" class="ui-btn-left">
                        <a href="#vehicleListLeftpanel" data-role="button" data-icon="bars" data-iconpos="notext" data-shadow="false" data-iconshadow="false" data-inline="true">Menu</a>
                </div>
                <div class="ui-btn-right" data-role="controlgroup" data-type="horizontal">
                    <a href="#fleetPage" data-role="button" data-icon="gear" data-iconpos="notext" data-shadow="false" data-iconshadow="false" data-inline="true">Fleets</a>                    
                
                </div>
                
            </div>
            <div data-role="content" id="fleetviewhome" style="padding-top:48px;">
                <div id="listviewwrapper">
                    <div data-role="fieldcontain" class="searchcontain">
                        <input name="vehicleListSearch" data-theme="d" id="vehicleListSearch" value="" data-clear-btn="true" type="text" placeholder="Search...">
                        <a style="float:right;margin-top:2px;margin-right:10px;" href="javascript:void(0)" onclick="searchVehicleList(CurrentFleetId);" data-role="button" data-icon="search" data-iconpos="notext" id="btnSearchVehicleList">Search</a>
                    </div>
                    <!--<ul data-role="listview" data-filter="true" data-scroll="true" data-filter-placeholder="Search" id="ulVehicleList">-->
                    <ul data-role="listview" data-filter="false" data-inset="false" data-theme="d" data-icon="false" data-filter-placeholder="Search..." class="jqm-list jqm-home-list" id="ulVehicleList">
                            
                    </ul>                 
                </div>
            </div>
            <div data-role="footer" data-position="fixed" data-fullscreen="true" data-tap-toggle="false">
		        <div data-role="navbar" data-grid="c">
			        <ul>
				        <!--<li><a href="/mobile/Home/FleetViewHome" id="menu" data-icon="bars" data-role="button" data-position-to="window" data-transition="flow" data-ajax="false">Vehicles</a></li>-->
                        <li><a href="#vehicleList" id="menuvl" data-icon="bars" data-role="button" data-rel="dialog" data-transition="none">Vehicles</a></li>
                        <li><a href="javascript:void(0)" onclick="$.mobile.changePage('#mappage');mapSelectedVehicle();" id="layersvl" data-icon="layers" data-role="button" data-rel="dialog" data-transition="none">Map</a></li>				    
                        <li><a href="#searchpage" onclick="PreviousPage = 'vehicleList';" id="searchvl" data-icon="search" data-role="button" data-transition="none">Search</a></li>				    
			        </ul>
		        </div>
	        </div>
            <div data-role="panel" id="vehicleListLeftpanel">
                <div data-position="inline" data-theme="a" data-role="header">
    <h1>Menu</h1>
</div>
<div data-role="content">
    <ul data-role="listview" data-filter="false">
        <li  data-icon="false"><a href="/mobile/Home" data-ajax="false">Home</a></li>  
        <li data-icon="false"><a href="/mobile/Account/Logoff" data-ajax="false">Logout</a></li>          
    </ul>
</div>                
            </div><!-- /panel -->            
    </div>
 


        
    <div data-role="page" id="searchpage" data-theme="d">
        <div data-role="header" data-theme="g">
            <h1>Search</h1>
            <a href="javascript:void(0)" onclick="$.mobile.changePage('#' + PreviousPage);" data-role="button" data-icon="delete" data-iconpos="notext" data-shadow="false" data-iconshadow="false" data-inline="true">Close</a>
        </div>
        <div data-role="content" data-theme="d" id="searchContent">
            <div data-role="fieldcontain" data-theme="d">
            <input type="search" name="query" id="query"
                    value="" placeholder="Search for places"
                    autocomplete="off"  data-theme="d"/>
            </div>
            <ul data-role="listview" data-inset="true" id="search_results" data-theme="d"></ul>
        </div>
    </div>
   

         

        
        <script type="text/javascript">
            var iniVehicle = null;
        </script>
    
    <script type="text/javascript">
        var currentVehicleListPageIndex = 1;
        var rootPath = "/mobile/";
        var loadVehicleByFleetPath = "/mobile/Home/_vehicleList/";
        var searchVehicleByFleetPath = "/mobile/Home/_searchVehicleList/";
        var VehicleListPageLazyLoading = false;
        var VehicleListPageLoadingFinished = false;
        var CurrentFleetId = 2174;
        var SearchMode = 0;
        var CurrentSearchString = '';
        var PreviousPage = 'vehicleList';
        

    </script>
    

